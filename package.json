{
  "name": "rakuten-tv",
  "version": "1.0.0",
  "description": "A small frontend clone of rakuten tv's web app",
  "main": "index.js",
  "proxy": "http://localhost:5000",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "create": "webpack",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaovcastro/rakuten-tv.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaovcastro/rakuten-tv/issues"
  },
  "homepage": "https://github.com/joaovcastro/rakuten-tv#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.2.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "compass-sass-stylesheets": "^1.0.3",
    "enzyme-adapter-react-16": "^1.11.2",
    "es6-arrow-function": "^0.6.6",
    "express": "^4.16.4",
    "express-favicon": "^2.0.1",
    "jquery": "^3.3.1",
    "path": "^0.12.7",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-lazyload": "^2.5.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "verbose": false,
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "./app",
      "node_modules"
    ],
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
